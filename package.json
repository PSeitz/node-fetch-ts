{
	"name": "node-fetch-ts",
	"version": "2.6.0",
	"description": "A light-weight module that brings window.fetch to node.js",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "types/index.d.ts",
	"files": [
		"src/**/*",
		"dist/**/*",
		"types/**/*.d.ts"
	],
	"engines": {
		"node": ">=10.0.0"
	},
	"scripts": {
		"build": "npx eslint . --ext .ts --quiet",
		"test": "mocha -r ts-node/register tests/**/*.spec.ts",
		"test_old": "cross-env BABEL_ENV=test mocha --require @babel/register --throw-deprecation test/test.js",
		"report_old": "cross-env BABEL_ENV=coverage nyc --reporter lcov --reporter text mocha -R spec test/test.js",
		"coverage": "cross-env BABEL_ENV=coverage nyc --reporter json --reporter text mocha -R spec test/test.js && codecov -f coverage/coverage-final.json",
		"lint": "xo"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/node-fetch/node-fetch.git"
	},
	"keywords": [
		"fetch",
		"http",
		"promise"
	],
	"author": "David Frank",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/node-fetch/node-fetch/issues"
	},
	"homepage": "https://github.com/node-fetch/node-fetch",
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/node-fetch"
	},
	"devDependencies": {
		"@babel/core": "^7.6.4",
		"@babel/preset-env": "^7.6.3",
		"@babel/register": "^7.6.2",
		"@types/chai": "^4.2.7",
		"@types/chai-as-promised": "^7.1.2",
		"@types/chai-string": "^1.4.2",
		"@types/mocha": "^5.2.7",
		"@types/utf8": "^2.1.6",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/typescript-estree": "^2.18.0",
		"abort-controller": "^3.0.0",
		"abortcontroller-polyfill": "^1.3.0",
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"chai-iterator": "^3.0.2",
		"chai-string": "^1.5.0",
		"codecov": "^3.6.1",
		"cross-env": "^6.0.3",
		"eslint": "^6.8.0",
		"mocha": "^7.0.0",
		"nyc": "^15.0.0",
		"parted": "^0.1.1",
		"prettier": "^1.19.1",
		"promise": "^8.0.3",
		"resumer": "0.0.0",
		"string-to-arraybuffer": "^1.0.2",
		"ts-node": "^8.6.2",
		"typescript": "^3.7.5",
		"xo": "^0.25.3"
	},
	"dependencies": {
		"data-uri-to-buffer": "^3.0.0",
		"fetch-blob": "^1.0.4",
		"form-data": "^3.0.0",
		"utf8": "^3.0.0"
	},
	"xo": {
		"envs": [
			"node",
			"browser"
		],
		"rules": {
			"valid-jsdoc": 0,
			"no-multi-assign": 0,
			"complexity": 0,
			"unicorn/prefer-spread": 0,
			"promise/prefer-await-to-then": 0,
			"no-mixed-operators": 0,
			"eqeqeq": 0,
			"no-eq-null": 0,
			"no-negated-condition": 0,
			"prefer-named-capture-group": 0,
			"unicorn/catch-error-name": 0,
			"node/no-deprecated-api": 1
		},
		"ignores": [
			"dist"
		],
		"overrides": [
			{
				"files": "test/**/*.js",
				"envs": [
					"node",
					"mocha"
				],
				"rules": {
					"max-nested-callbacks": 0,
					"no-unused-expressions": 0,
					"eslint-comments/no-unused-disable": 0,
					"new-cap": 0,
					"guard-for-in": 0,
					"no-new": 0
				}
			},
			{
				"files": "example.js",
				"rules": {
					"import/no-extraneous-dependencies": 0
				}
			}
		]
	},
	"nyc": {
		"require": [
			"@babel/register"
		],
		"sourceMap": false,
		"instrument": false
	},
	"runkitExampleFilename": "example.js"
}
